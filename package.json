{
  "name": "emotion-ts-plugin",
  "version": "1.1.0",
  "description": "TypeScript Emotion Plugin",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/Brooooooklyn/emotion-ts-plugin",
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir lib --diagnostics && tsc -p tsconfig.json -m esnext --outDir esm --diagnostics",
    "dev": "tsc -p tsconfig.json --outDir lib --diagnostics -w",
    "format": "run-p format:md format:json format:source format:yml",
    "format:json": "prettier --parser json --write '**/*.json'",
    "format:md": "prettier --parser markdown --write './*.md'",
    "format:source": "prettier --config ./package.json './{src,tests}/**/*.{ts,tsx,js}' --write",
    "format:yml": "prettier --parser yaml --write './*.{yml,yaml}'",
    "lint": "eslint . -c ./.eslintrc.yml '{src,tests}/**/*.{js,ts,tsx}'",
    "start": "webpack serve --progress --color",
    "test": "jest --no-cache --ci",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@swc-node/jest": "^1.3.1",
    "@types/convert-source-map": "^1.5.2",
    "@types/find-root": "^1.1.2",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "7",
    "jest": "^27.0.6",
    "jest-specific-snapshot": "^5.0.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.2.4",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@emotion/hash": "^0.8.0",
    "convert-source-map": "^1.8.0",
    "find-root": "^1.1.0",
    "lodash": "^4.17.21",
    "source-map": "^0.7.3",
    "tslib": "^2.3.1"
  },
  "files": ["lib/**", "esm/**"],
  "jest": {
    "preset": "@swc-node/jest",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
    "testRegex": "/tests/.*\\.spec\\.(j|t)sx?$",
    "collectCoverage": true,
    "collectCoverageFrom": ["src/**/*.ts"]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": ["prettier --write", "eslint -c .eslintrc.yml --fix"],
    "*.@(yml|yaml)": ["prettier --parser yaml --write"],
    "*.md": ["prettier --parser markdown --write"],
    "*.json": ["prettier --parser json --write"]
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  }
}
