{
  "name": "emotion-ts-plugin",
  "version": "2.0.1",
  "description": "TypeScript Emotion Plugin",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/Brooooooklyn/emotion-ts-plugin",
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir lib --diagnostics && tsc -p tsconfig.json -m esnext --outDir esm --diagnostics",
    "dev": "tsc -p tsconfig.json --outDir lib --diagnostics -w",
    "format": "prettier . --write",
    "lint": "eslint . -c ./.eslintrc.yml",
    "start": "webpack serve --progress --color",
    "test": "jest --no-cache --ci",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@swc-node/jest": "^1.4.3",
    "@types/convert-source-map": "^2.0.0",
    "@types/find-root": "^1.1.2",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.180",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "8",
    "jest": "^29.0.0",
    "jest-specific-snapshot": "^8.0.0",
    "lint-staged": "^13.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.6.0",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-loader": "^9.2.8",
    "typescript": "^5.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@emotion/hash": "^0.9.0",
    "convert-source-map": "^2.0.0",
    "find-root": "^1.1.0",
    "lodash": "^4.17.21",
    "source-map": "^0.7.3",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "typescript": ">= 4.2"
  },
  "files": [
    "lib/**",
    "esm/**"
  ],
  "jest": {
    "preset": "@swc-node/jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "/tests/.*\\.spec\\.(j|t)sx?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "eslint -c .eslintrc.yml --fix"
    ],
    "*.@(js|ts|tsx|yml|yaml|md|json)": [
      "prettier --write"
    ]
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Brooooooklyn"
  }
}
